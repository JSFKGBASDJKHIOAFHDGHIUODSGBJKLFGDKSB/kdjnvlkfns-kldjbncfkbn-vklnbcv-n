local function isPlayerWhitelisted(PlayerID)
    for _, whitelistedUser in ipairs(whitelistedPlayers) do
        if tostring(playerID) == whitelistedUser then
            return true
        end
    end
    return false
end

local function showNotification(message, backgroundColor)
    local gui = Instance.new("ScreenGui")
    gui.Name = "Notification"
    gui.Parent = player.PlayerGui
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.BackgroundColor3 = backgroundColor
    frame.Parent = gui
    local messageLabel = Instance.new("TextLabel")
    messageLabel.Size = UDim2.new(1, 0, 1, 0)
    messageLabel.Position = UDim2.new(0, 0, 0, 0)
    messageLabel.Text = message
    messageLabel.TextSize = 36
    messageLabel.Parent = frame
    repeat
        wait(1)
    until isPlayerWhitelisted(playerID)

    gui:Destroy()
end

