    function SendMessage(url, message)
        local http = game:GetService("HttpService")
        local headers = {
            ["Content-Type"] = "application/json"
        }

        local data = {
            ["username"] = botname,
            ["content"] = message
        }

        local body = http:JSONEncode(data)
        local response =
            request(
            {
                Url = url,
                Method = "POST",
                Headers = headers,
                Body = body
            }
        )
    end

    function SendMessageEMBED(url, embed)
        local http = game:GetService("HttpService")
        local headers = {
            ["Content-Type"] = "application/json"
        }

        local data = {
            ["username"] = botname,
            ["embeds"] = {
                {
                    ["title"] = embed.title,
                    ["description"] = embed.description,
                    ["color"] = embed.color,
                    ["fields"] = embed.fields,
                    ["footer"] = {
                        ["text"] = embed.footer.text
                    }
                }
            }
        }

        local body = http:JSONEncode(data)
        local response =
            request(
            {
                Url = url,
                Method = "POST",
                Headers = headers,
                Body = body
            }
        )
    end

